{
    "compilerOptions": {
      "target": "es6",
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "skipLibCheck": true,
      "allowJs": true,
      "outDir": "dist",
      "baseUrl": ".",
      "paths": {
          "*": ["node_modules/*", "src/types/*"]
      },
      "sourceMap": true,
      "noImplicitAny": true,
      "allowSyntheticDefaultImports": true,
    }
}
